{
    "name": "nodebackend",
    "version": "1.0.0",
    "private": true,
    "description": "Starter kit for all Node.JS Back Ends",
    "repository": "https://github.com/apelttom/BE-nodeJS-starter",
    "license": "MIT",
    "author": "Tom Apeltauer",
    "main": "server.js",
    "scripts": {
        "build": "yarn clean && tsc && yarn copy-files",
        "clean": "rimraf dist/",
        "copy-files": "copyfiles -u 1 src/**/*.html dist/src/",
        "dev": "ts-node ./src/server.ts",
        "health-check": "yarn lint --max-warnings 0 && yarn ts-check",
        "lint": "eslint --ext src/**/*.ts",
        "lint:fix": "sort-package-json \"package.json\" \"packages/**/package.json\"  && prettier --write \"**/*.{css,scss,less,md,json,html,ts,yaml}\" --config ./.prettierrc.json && yarn lint --fix && yarn ts-check",
        "newclean": "find . | grep /node_modules$ | grep -v /node_modules/ | xargs rm -fR \\; &&  yarn cache clean && yarn",
        "start": "node ./dist/src/server.js",
        "test": "jest",
        "ts-check": "yarn tsc --project ./tsconfig.json"
    },
    "dependencies": {
        "@babel/core": "^7.22.9",
        "@babel/preset-env": "^7.22.9",
        "@types/express": "^4.17.17",
        "@types/express-actuator": "^1.8.0",
        "@types/node": "^18.16.19",
        "core-js": "^3.31.1",
        "dirty": "^1.1.3",
        "dotenv": "^16.3.1",
        "eslint": "^8.45.0",
        "express": "^4.18.2",
        "express-actuator": "^1.8.4",
        "express-async-errors": "^3.1.1",
        "jest-environment-node": "^29.6.1",
        "mongodb": "^5.7.0",
        "mongoose": "^7.4.0",
        "prom-client": "^14.2.0",
        "redis-om": "^0.4.0",
        "sucrase": "^3.34.0",
        "typescript": "^5.1.6",
        "uuid": "^9.0.0"
    },
    "devDependencies": {
        "@babel/preset-typescript": "^7.22.5",
        "@shelf/jest-mongodb": "^4.1.7",
        "@sucrase/jest-plugin": "^3.0.0",
        "@types/jest": "^29.5.3",
        "@types/uuid": "^9.0.2",
        "@typescript-eslint/eslint-plugin": "^5.62.0",
        "@typescript-eslint/parser": "^5.62.0",
        "copyfiles": "^2.4.1",
        "eslint-config-prettier": "^8.8.0",
        "eslint-config-standard-with-typescript": "^36.1.0",
        "eslint-plugin-import": "^2.27.5",
        "eslint-plugin-n": "^16.0.1",
        "eslint-plugin-promise": "^6.1.1",
        "jest": "^29.6.1",
        "prettier": "3.0.0",
        "rimraf": "^5.0.1",
        "sort-package-json": "^2.5.1",
        "ts-jest": "^29.1.1",
        "ts-node": "^10.9.1"
    },
    "packageManager": "yarn@3.6.1"
}
