{
    "name": "nodebackend",
    "version": "1.0.0",
    "private": true,
    "description": "Starter kit for all Node.JS Back Ends",
    "repository": "https://github.com/apelttom/BE-nodeJS-starter",
    "license": "MIT",
    "author": "Tom Apeltauer",
    "main": "server.js",
    "scripts": {
        "build": "yarn clean && tsc && yarn copy-files",
        "clean": "rimraf dist/",
        "copy-files": "copyfiles -u 1 src/**/*.html src/**/*.css dist/src/",
        "dev": "ts-node ./src/server.ts",
        "health-check": "yarn lint --max-warnings 0 && yarn ts-check",
        "lint": "eslint --ext src/**/*.ts",
        "lint:fix": "sort-package-json \"package.json\" \"packages/**/package.json\"  && prettier --write \"**/*.{css,scss,less,md,json,html,ts,yaml}\" --config ./.prettierrc.json && yarn lint --fix && yarn ts-check",
        "newclean": "find . | grep /node_modules$ | grep -v /node_modules/ | xargs rm -fR \\; &&  yarn cache clean && yarn",
        "start": "node ./dist/src/server.js",
        "test": "jest",
        "ts-check": "yarn tsc --project ./tsconfig.json"
    },
    "dependencies": {
        "@babel/core": "^7.0.0-0",
        "@babel/preset-env": "^7.20.2",
        "@types/express": "^4.17.17",
        "@types/node": "^18.11.19",
        "core-js": "^3.29.1",
        "crypto": "^1.0.1",
        "dirty": "^1.1.3",
        "dotenv": "^16.0.3",
        "eslint": "^8.0.1",
        "express": "^4.18.2",
        "express-async-errors": "^3.1.1",
        "jest-environment-node": "^29.5.0",
        "mongodb": "^3",
        "mongoose": "^7.0.1",
        "redis-om": "^0.3.6",
        "sucrase": "^3.29.0",
        "typescript": "*"
    },
    "devDependencies": {
        "@babel/preset-typescript": "^7.21.0",
        "@shelf/jest-mongodb": "^4.1.7",
        "@sucrase/jest-plugin": "^3.0.0",
        "@types/jest": "^29.4.1",
        "@typescript-eslint/eslint-plugin": "^5.0.0",
        "@typescript-eslint/parser": "^5.51.0",
        "copyfiles": "^2.4.1",
        "eslint-config-prettier": "^8.7.0",
        "eslint-config-standard-with-typescript": "^34.0.0",
        "eslint-plugin-import": "^2.25.2",
        "eslint-plugin-n": "^15.0.0",
        "eslint-plugin-promise": "^6.0.0",
        "jest": "^29.5.0",
        "prettier": "2.8.4",
        "rimraf": "^5.0.0",
        "sort-package-json": "^2.4.1",
        "ts-jest": "^29.0.5",
        "ts-node": "^10.9.1"
    }
}
